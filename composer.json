{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-zip": "*",
        "authorizenet/authorizenet": "^2.0",
        "bacon/bacon-qr-code": "^3.0",
        "doctrine/dbal": "^3.5",
        "google/apiclient": "^2.18",
        "guzzlehttp/guzzle": "^7.2",
        "iyzico/iyzipay-php": "^2.0",
        "jubaer/zoom-laravel": "^1.3",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2",
        "laravel/socialite": "^5.6",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^4.1",
        "league/flysystem-aws-s3-v3": "^3.16",
        "mercadopago/dx-php": "^2.5",
        "mollie/laravel-mollie": "3.0",
        "omnipay/paypal": "^3.0",
        "paytm/paytmchecksum": "^1.1",
        "php-http/guzzle7-adapter": "^1.0",
        "php-http/message-factory": "^1.1",
        "pragmarx/google2fa-laravel": "^2.2.0",
        "pusher/pusher-php-server": "^7.2",
        "razorpay/razorpay": "^2.8",
        "spatie/laravel-activitylog": "^4.8",
        "spatie/laravel-cookie-consent": "^3.2",
        "spatie/laravel-permission": "^6.1",
        "stripe/stripe-php": "^9.6",
        "twilio/sdk": "^7.3",
        "vrajroham/laravel-bitpay": "^6.0",
        "yajra/laravel-datatables-oracle": "^10.2",
        "yansongda/pay": "^3.7",
        "zainiklab/zai-installer": "^1.9"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.9",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/Helper.php",
            "app/Helpers/Constant.php",
            "app/Helpers/CoreArray.php",
            "app/Helpers/LangHelper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
